<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style>
	.footer {
		background : hsla(120, 100%, 0%, 0.3);
		max-width: 100%;
		height: 50px;
		left: 0;
		right: 0;
		bottom: 0;
		position : fixed;
		overflow: auto;
		margin: 0px;
		text-align: center;
	}
	
	.sns{
		height: 35px;
		margin-top: 5px;
	}
	
	.sns:hover{
		height: 35px;
		margin-top: 5px;
		background: white;
	}
</style>
</head>
<body>
	<div class="footer">
		<img src="img/twitter.png" class="sns">
		<img src="img/facebook.png" class="sns">
		<img src="img/youtube.png" class="sns">
	</div>
</body>
</html>